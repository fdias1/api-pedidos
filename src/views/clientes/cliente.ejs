<h1>Cliente</h1>
<% if(!cliente) { %>
    <% cliente = {nome:'',telefone:'',endereco:[]} %>
<% } %>  
<form action="<%= cliente.telefone %>/editar" method="POST">
    <div>
        <label for="nome">Nome:</label>
        <input type="text" name="nome" id="nome" value=<%= cliente.nome %> >
    </div>
    <div>
        <label for="telefone">Telefone:</label>
        <input type="text" name="telefone" id="telefone" value=<%= cliente.telefone %>> 
    </div>
    <button action="submit">Editar</button> 
</form>